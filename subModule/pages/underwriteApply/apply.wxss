page{
   background: #f2f2f2; 
   font-size: 30rpx;
   color: #999;
}
.contain{
  background: #fff;
}
.top-bar{
  background: #FFFAF3;
  font-size: 25rpx;
  color:#333;
  padding: 24rpx 20rpx;
  line-height: 36rpx;
  display:flex;
  flex-direction:row;
  justify-content:flex-start;  
}
.top-bar text{
  line-height: 36rpx;
  flex: 1;
}
.top-bar-tip-icon{
  width: 28rpx;
  height: 28rpx;
  margin-right: 12rpx;
}
.underwrite-apply-product{
  border-bottom:1rpx solid rgba(0,0,0,0.1);
}
.underwrite-apply-product .underwrite-state-box-btn{
  display: none;
}
.underwrite-apply-column{
  display:flex;
  flex-direction:row;
  /* justify-content:space-between; */
  align-items:center;
  padding: 24rpx;
  border-bottom:1rpx solid rgba(0,0,0,0.1);
}
.area-column{
  align-items:flex-start;
}
.underwrite-apply-column .title{
  width: 150rpx;
}
.column-flex{
  flex: 1;
  display:flex;
  color: #333;
}
.time-select-icon{
  width:50rpx;
  height:50rpx;
}
.column-flex-item{
  flex: 1;
  display:flex;
  flex-direction:row;
  justify-content:flex-start;
  align-items:center;
}
.underwrite-submit{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  background: #E53935;
  height: 98rpx;
  line-height: 98rpx;
  text-align: center;
  font-size: 30rpx;
  color:#fff;
}
.area-column textarea{
  height: 120rpx;
}