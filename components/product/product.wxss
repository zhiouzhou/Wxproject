.flex-display {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.shop-cart-product-edit-column {
  display: flex;
  display: -moz-box;
  display: -webkit-box;
  -moz-box-align: center;
  -webkit-box-align: center;
  -o-box-align: center;
  box-align: center;
}

.shop-cart-product-edit-column-spec {
  box-flex: 1;
  -webkit-box-flex: 1;
  -moz-box-flex: 1;
  display: block;
}

.shop-cart-product-edit-column-spec-frame {
  width: 196rpx;
  height: 58rpx;
  border: 2rpx solid #bdbdbd;
  box-sizing: border-box;
  border-radius: 4rpx;
  margin-right: 16rpx;
  overflow: hidden;
}

.product-wrap-column {
  display: flex;
  display: -moz-box;
  display: -webkit-box;
  -moz-box-align: center;
  -webkit-box-align: center;
  -o-box-align: center;
  box-align: center;
}

.product-wrap-column-spec {
  box-flex: 1;
  -webkit-box-flex: 1;
  -moz-box-flex: 1;
  display: block;
}

.product-tag-column >.product-tag-wrap {
  display: none;
}

.product-tag-column >.product-tag-wrap:nth-of-type(1) {
  display: flex;
}

.product-tag-column >.product-tag-wrap:nth-of-type(2) {
  display: flex;
}

