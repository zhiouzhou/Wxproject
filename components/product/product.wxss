.flex {
  display: flex;
}

.activity-product-wrap {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: stretch;
}

.normal-product-wrap {
  width: 100%;
  background-color: #fff;
  padding: 16rpx 24rpx 16rpx 16rpx;
  box-sizing: border-box;
  border-top: 2rpx solid rgba(0, 0, 0, 0.1);
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: flex-start;
}

.product-left-side {
  width: 192rpx;
  margin-right: 16rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
}

.product-img-area {
  position: relative;
  width: 192rpx;
  height: 192rpx;
}

.product-img {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
}

.product-promotion-img {
  position: absolute;
  top: 0;
  left: 0;
  width: 80rpx;
  height: 80rpx;
}

.product-promotion-img-small {
  position: absolute;
  top: 0;
  left: 0;
  width: 60rpx;
  height: 60rpx;
}

.product-promotion-tag {
  width: 26rpx;
  height: 26rpx;
  background-color: #e53935;
  color: #fff;
  font-size: 20rpx;
  line-height: 26rpx;
  text-align: center;
  border-radius: 4rpx;
}

.product-reduce {
  position: absolute;
  top: 0;
  right: 0;
  width: 60rpx;
  height: 60rpx;
  background-color: #e53935;
  border-radius: 50%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.product-stock {
  position: absolute;
  width: 100%;
  height: 26rpx;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.4);
  text-align: center;
  line-height: 26rpx;
  color: #fff;
  font-size: 20rpx;
}

.product-production-date {
  margin-top: 16rpx;
  height: 26rpx;
  width: 100%;
  background-color: #f2f2f2;
  text-align: center;
  line-height: 26rpx;
  color: #666;
  font-size: 20rpx;
}

.product-right-side {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: stretch;
}

.product-main-info {
  min-height: 192rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: stretch;
}

.product-price-area {
  flex: 1;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  position: relative;
}

.product-price-pj {
  margin: 10rpx 0 4rpx;
}

.product-price-area .ft-36 {
  margin: 0 4rpx;
}

.product-price-pj .ft-36 {
  margin: 0 4rpx;
}

.member-price {
  position: absolute;
  right: 24rpx;
}

.member-tag {
  display: inline-block;
  background-color: #333;
  color: #fff;
  margin-left: 4px;
  font-size: 20rpx;
  padding: 0 2px;
}

.product-spec-wrap {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.product-number-frame {
  width: 196rpx;
  height: 58rpx;
  display: inline-flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  border: 2rpx solid #bdbdbd;
  box-sizing: border-box;
  border-radius: 4rpx;
  margin-right: 16rpx;
  position: relative;
  z-index: 2;
}
.product-number-frame:after{
  position: absolute;
  content: "";
  width: 150%;
  height: 300%;
  left: -25%;
  top: -100%;
  background: rgba(0,0,0,0);
  overflow:initial;
  z-index: -1;
}

.product-arrival-notice {
  height: 48rpx;
