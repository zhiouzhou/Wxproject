

.cp-underwrite-h5{
	background:#E53935 !important;
	color:#fff;
  border-top-right-radius: 10px;
  border-top-left-radius: 10px;
  width:100%;
  line-height: 90rpx;
  font-size:32rpx;
}
.dialog-cp-explain .cp-underwrite-h5{
    font-size: 34rpx;
    font-weight: 400;
    padding: 0.2rem 0;
    margin:0 !important;
    background:#E53935 !important;
}
.cp-underwrite{
    font-size: 28rpx;
    text-align: left;
    padding: 0 50rpx;
    margin-bottom: 5px;
}
.cp-underwrite-title{
	margin-top:10px;
	color:#333;
	font-size:28rpx;
	text-align:center;
}
.cp-underwrite-detail{
	color:#666;
	font-size:28rpx;
	margin:16rpx 0;	
}
.cp-underwrite-list{
	font-size:28rpx;
	color:#666;
}
.cp-underwrite-list .cp-underwrite-list-item{
	display:flex; display:-moz-box; display:-webkit-box;
	-moz-box-align:center;-webkit-box-align:center;-o-box-align:center;box-align:center;  
	margin-top:5px;
}
.cp-underwrite-list-item image{
  width: 56rpx;
  height: 56rpx;
	margin-right:30rpx;
}
.cp-underwrite-list-item text{
	box-flex:1; -webkit-box-flex:1; -moz-box-flex:1;display:block;
}
.cp-explain-btn{
  border-top:1rpx solid rgba(0,0,0,0.1);
  width: 100%;
  line-height: 90rpx;
  color:#E53935;
  font-size:32rpx;
  box-sizing: border-box;
}
